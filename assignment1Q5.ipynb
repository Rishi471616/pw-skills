    "A = int(input())\n",
    "B = int(input())\n",
    "\n",
    "count = 0\n",
    "\n",
    "while A%B ==0:\n",
    "    A = A/B\n",
    "    count += 1\n",
    "\n",
    "if count > 0:\n",
    "    print (f\" {A} can be divided by {B} in {count} times \")\n",
    "else:\n",
    "        print(f\"{A} cannot be divided by {B}\")"
